@font-face {
    font-family: 'Supreme LL TT';
    src: url({{ 'SupremeLLTT-Bold.eot' | asset_url }});
    src: url({{ 'SupremeLLTT-Bold.eot' | asset_url }}?#iefix) format('embedded-opentype'),
        url({{ 'SupremeLLTT-Bold.woff2' | asset_url }}) format('woff2'),
        url({{ 'SupremeLLTT-Bold.woff' | asset_url }}) format('woff');
    font-weight: bold;
    font-style: normal;
    font-display: swap;
}

